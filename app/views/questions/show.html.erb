<%= form_with(model: [@entry, @question]) do |form| %>
  <div class="form-floating mb-3">
    <%= form.text_field :text, placeholder: 'Quiz Question', class: 'form-control' %>
    <%= form.label :text, "Quiz Question" %>
  </div>

  <%= form.submit "+ Create New Quiz Question", class: "btn btn-primary" %>
<% end %>
